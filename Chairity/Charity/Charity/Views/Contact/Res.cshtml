
@{
    ViewBag.Title = "Res";
}

<section class="inner-header divider parallax layer-overlay overlay-dark-5" data-stellar-background-ratio="0.5" data-bg-img="/Content/images/bg/bg1.jpg">
    <div class="container pt-100 pb-50">
        <!-- Section Content -->
        <div class="section-content pt-100">
            <div class="row">
                <div class="col-md-12">
                    <h3 class="title text-white">Contact</h3>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Divider: Contact -->
<section class="divider">
    <div class="container pt-0">
        <div class="row mb-60 bg-deep">
           @{ 
               if (Session["_c"] != null)
               {
                   if (Session["_c"].ToString() == "true")
                   {
                       <h3 style="color:black;">Thank you for contact us, we will get back to you soon</h3>
                   }

                   if (Session["_c"].ToString() == "false")
                   {
                    <h3 style="color:black;">Error occured while contacting us. please try again later.</h3>
                   }

                   Session["_c"] = null;
               }
        }
        </div>
 
    </div>
</section>


